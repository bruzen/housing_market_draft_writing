@article{tomaskovic-deveyFinancializationCausesInequality2013,
  title = {Financialization: {{Causes}}, {{Inequality Consequences}}, and {{Policy Implications}}},
  author = {{Tomaskovic-Devey}, Donald and Lin, Ken-Hou},
  year = {2013},
  journal = {NC Banking Inst},
  volume = {18},
  pages = {167--194},
  abstract = {The U.S. is now a financializedeconomy, where the financial sector and its priorities have become increasingly dominant in all aspects of the economy. We focus on financializationas a process of income redistribution with two faces. The first face is one of rent seeking by an increasingly concentrated and politically influential finance sector. This rent seeking has been successful, leading to the pooling ofprofits and income in thefinance sector.The secondface is a shift in behavior of non-financefirms awayfrom production and non- financialservices and towardfinancial investments and services. This shift has had both strategicand normative components and has reduced the bargainingpower of labor and the centrality ofproduction. As a consequence,financializationof the non-finance sector has led to lower employment, income transfers to executives and capital owners, and increasedinequality among workers. We discuss the policy implications ofthese consequencesatthe end ofthisArticle.},
