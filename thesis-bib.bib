@article{anderssonSourcesUrbanWage2014,
  title = {The Sources of the Urban Wage Premium by Worker Skills: {{Spatial}} Sorting or Agglomeration Economies?1},
  shorttitle = {The Sources of the Urban Wage Premium by Worker Skills},
  author = {Andersson, Martin and Klaesson, Johan and Larsson, Johan P},
