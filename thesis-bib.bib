@article{dongUnderstandingMesoscopicScaling2020,
  title = {Understanding the {{Mesoscopic Scaling Patterns Within Cities}}},
  author = {Dong, Lei and Huang, Zhou and Zhang, Jiang and Liu, Yu},
  year = {2020},
