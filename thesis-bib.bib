@article{beaudryWhoRightMarshall2009,
  title = {Who's Right, {{Marshall}} or {{Jacobs}}? {{The}} Localization versus Urbanization Debate},
  author = {Beaudry, Catherine and Schiffauerova, Andrea},
  year = {2009},
  journal = {Research Policy},
  volume = {38},
  number = {2},
  pages = {318--337},
  publisher = {{Elsevier}},
  doi = {10.1016/j.respol.2008.11.010},
  abstract = {Abstract A large amount of literature provides empirical evidence in support of Marshall or Jacobs theories regarding the specialization or diversity effects on the economic performance of regions. This paper surveys these scholarly contributions and summarizes their results according to their similarities and differences. The reviewed empirical work presents a diverse picture of possible conditions and circumstances under which each kind of externalities could be at work. The wide breadth of findings is generally not explained by differences in the strength of agglomeration forces across industries, countries or time periods, but by measurement and methodological issues. The levels of industrial and geographical aggregation together with the choice of performance measures, specialization and diversity indicators are the main causes for the lack of resolution in the debate. The 3-digit industrial classification seems to be the level at which MAR and Jacobs effects are undistinguishable from one another, and this is often exacerbated by a high level of geographical aggregation.},
  date-added = {2021-03-12 13:49:14 -0500},
  date-modified = {2021-06-26 17:54:11 -0400},
  ty = {JOUR},
  keywords = {Agglomeration externalities,Geographical aggregation,Growth,Industrial classification,Innovation,O30,R11,R12}
}

@techreport{belderbosWhatSpilloversUniversities2022,
